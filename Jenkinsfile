pipeline{
  agent any{
    
     stages{
        stage("Build")
         {
          steps{
            script{
               echo "INFO: BUILD STAGE"
                  }
        
               }
         }
       
      stage("Test")
      {
         steps{
           script{
            echo "INFO: TEST STAGE"
                 }
              }
      }
      stage("Deploy")
       {
        steps{
          script{
            echo "INFO: DEPLOY STAGE"
                }
        }
             }
       }
           }
}
      
      
        
          
